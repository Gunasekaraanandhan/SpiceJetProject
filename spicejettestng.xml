<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="com.spicejet.utils.Reports" />
	</listeners>
	  
	<test thread-count="5" name="ChromeTest">
		<parameter name="browser" value="chrome" />
		<classes>

			<class name="com.spicejet.utils.Wrapper" />
			<class name="com.spicejet.utils.Reports" />

			<class name="com.spicejet.test.SignupTest" />
			<class name="com.spicejet.test.PositiveLoginTest" />
			<class name="com.spicejet.test.NegativeLoginTest" />
			<class name="com.spicejet.test.OneWayTripTest" />
			<class name="com.spicejet.test.RoundTripTest" />
			<class name="com.spicejet.test.CheckInValidationTest" />
			<class name="com.spicejet.test.FlightStatusValidationTest" />
			<class name="com.spicejet.test.ManageBookingTest" />

		</classes>
	</test> <!-- Test -->
	

	<test thread-count="5" name="EdgeTest">
		<parameter name="browser" value="edge" />
		<classes>

			<class name="com.spicejet.utils.Wrapper" />
			<class name="com.spicejet.utils.Reports" />

			<class name="com.spicejet.test.SignupTest" />
			<class name="com.spicejet.test.PositiveLoginTest" />
			<class name="com.spicejet.test.NegativeLoginTest" />
			<class name="com.spicejet.test.OneWayTripTest" />
			<class name="com.spicejet.test.RoundTripTest" />
			<class name="com.spicejet.test.CheckInValidationTest" />
			<class name="com.spicejet.test.FlightStatusValidationTest" />
			<class name="com.spicejet.test.ManageBookingTest" />

		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
